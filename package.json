{
  "name": "solid-starter-kit",
  "version": "1.0.0",
  "description": "Solid.js starter kit with common structure from pays.online and honey.id",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pathscale/ui-starter-app"
  },
  "scripts": {
    "start": "rsbuild dev",
    "dev": "rsbuild dev",
    "build": "npm run typecheck && rsbuild build && bun run cleanup",
    "typecheck": "tsc --noEmit",
    "preview": "rsbuild preview",
    "cleanup": "bun run src/scripts/cleanup.js",
    "prepare": "husky",
    "lint": "biome check .",
    "format": "biome format . --write",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "biome check --apply"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@felte/solid": "^1.2.14",
    "@felte/validator-zod": "^1.0.18",
    "@iconify/tailwind4": "^1.0.6",
    "@modular-forms/solid": "^0.25.1",
    "@pathscale/ui": "^0.0.94",
    "@solid-primitives/refs": "^1.1.1",
    "@solidjs/router": "^0.15.3",
    "@tanstack/solid-query": "^5.76.0",
    "@tanstack/solid-query-devtools": "^5.90.3",
    "@tanstack/solid-table": "^8.21.3",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.43",
    "glob": "^11.0.3",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.5",
    "solid-sonner": "^0.2.8",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@rsbuild/core": "^1.3.20",
    "@rsbuild/plugin-babel": "^1.0.5",
    "@rsbuild/plugin-solid": "^1.0.5",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^22.15.17",
    "biome": "^0.3.3",
    "class-variance-authority": "^0.7.1",
    "commitizen": "^4.3.0",
    "cssnano": "^7.0.7",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "tailwindcss": "^4.0.7",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.7.2"
  }
}
